\begin_layout Standard
This form is important because it relates 
\begin_inset Formula $A$
\end_inset

 and 
\begin_inset Formula $B$
\end_inset

 to the coefficients.
 To get the coefficients, this form must be converted to FORM 2 given by
 Kaps and Rentrop [REF].
 This form is given as
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathbf{y}_{1}=\mathbf{y}_{0}+\sum_{i=1}^{s}\widetilde{m}_{i}\widetilde{\mathbf{k}}_{i},\label{eq:form2y}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\left(\mathbb{I}-\gamma h\mathbb{F}_{y}\left(x_{0},\mathbf{y}_{0}\right)\right)\widetilde{\mathbf{k}}_{i}=h\mathbf{f}\left(x_{0}+A_{i}h,\mathbf{y}_{0}+\sum_{j=1}^{i-1}\widetilde{a}_{ij}\widetilde{\mathbf{k}}_{j}\right)+B_{i}h\mathbf{f}_{x}\left(x_{0},\mathbf{y}_{0}\right)+h\mathbb{F}_{y}\left(x_{0},\mathbf{y}_{0}\right)\sum_{j=1}^{i-1}\widetilde{c}_{ij}\widetilde{\mathbf{k}}_{j}.\quad\left(i=1\ldots s\right)\label{eq:form2k}
\end{equation}

\end_inset

This form is important because these are the coefficients that will be calculate
d.
 They can then be transformed to FORM 1 to compute 
\begin_inset Formula $A$
\end_inset

 and 
\begin_inset Formula $B$
\end_inset

.
 Finally, the form present in Numerical Recipes, referred to as FORM 3 is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathbf{y}_{1}=\mathbf{y}_{0}+\sum_{i=1}^{s}\hat{m}_{i}\hat{\mathbf{k}}_{i},\label{eq:form3y}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\left(\mathbb{I}/\left(\gamma h\right)-\mathbb{F}_{y}\left(x_{0},\mathbf{y}_{0}\right)\right)\mathbf{\hat{k}}_{i}=\mathbf{f}\left(x_{0}+A_{i}h,\mathbf{y}_{0}+\sum_{j=1}^{i-1}\widehat{a}_{ij}\mathbf{\hat{k}}_{j}\right)+B_{i}h\mathbf{f}_{x}\left(x_{0},\mathbf{y}_{0}\right)+\frac{1}{h}\sum_{j=1}^{i-1}\hat{c}_{ij}\mathbf{\hat{k}}_{j}.\quad\left(i=1\ldots s\right)\label{eq:form3k}
\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Convert from FORM 2 to FORM 1
\end_layout

\begin_layout Standard
To convert from FORM 2 to FORM 1 the following relationship is used:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathbf{k}_{i}=\frac{\mathbf{\widetilde{k}}_{i}}{h}+\sum_{j}^{i-1}c_{ij}\mathbf{k}_{i}.
\end{equation}

\end_inset

 This equation can be substituted into Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:form1k"

\end_inset

 such that
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline}
\left(\mathbb{I}-\gamma h\mathbb{F}_{y}\left(x_{0},\mathbf{y}_{0}\right)\right)\left(\frac{\mathbf{\widetilde{k}}_{i}}{h}+\sum_{j}^{i-1}c_{ij}\mathbf{k}_{i}\right)=\mathbf{f}\left(x_{0}+A_{i}h,\mathbf{y}_{0}+h\sum_{j=1}^{i-1}a_{ij}\left(\frac{\mathbf{\widetilde{k}}_{j}}{h}+\sum_{k}^{j-1}c_{jk}\mathbf{k}_{j}\right)\right)\\
+B_{i}h\mathbf{f}_{x}\left(x_{0},\mathbf{y}_{0}\right)+\sum_{j=1}^{i-1}c_{ij}\left(\frac{\mathbf{\widetilde{k}}_{j}}{h}+\sum_{k}^{j-1}c_{jk}\mathbf{k}_{k}\right),\quad\left(i=1\ldots s\right)
\end{multline}

\end_inset

 Rearranging this equation 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline}
\mathbf{\widetilde{k}}_{i}=h\mathbf{f}\left(x_{0}+A_{i}h,\mathbf{y}_{0}+h\sum_{j=1}^{i-1}a_{ij}\left(\frac{\mathbf{\widetilde{k}}_{i}}{h}+\sum_{k}^{j-1}c_{jk}\mathbf{k}_{k}\right)\right)+\gamma h\mathbb{F}_{y}\left(x_{0},\mathbf{y}_{0}\right)\mathbf{\widetilde{k}}_{i}-h\sum_{j=1}^{i-1}c_{ij}\mathbf{k}_{j}+B_{i}h\mathbf{f}_{x}\left(x_{0},\mathbf{y}_{0}\right)\\
-\gamma h^{2}\mathbb{F}_{y}\left(x_{0},\mathbf{y}_{0}\right)\sum_{j=1}^{i-1}c_{ij}\mathbf{k}_{j}+h\sum_{j=1}^{i-1}c_{ij}\left(\frac{\mathbf{\widetilde{k}}_{j}}{h}+\sum_{k=1}^{j-1}c_{jk}\mathbf{k}_{k}\right).\label{eq:2to1}
\end{multline}

\end_inset

 Notice that in this equation 
\begin_inset Formula $\mathbf{k}$
\end_inset

 still shows up.
 This will be eliminated recursively as the equations are written.
 First, we concentrate on the 
\begin_inset Formula $a_{ij}$
\end_inset

 terms and relate them between FORM 1 and FORM 2.
 We now write the equations for each 
\begin_inset Formula $i$
\end_inset

 comparing FORM 1 and 2:
\end_layout

\begin_layout Paragraph
Equation 1
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathbf{y}_{0}=\mathbf{y}_{0}
\end{equation}

\end_inset


\end_layout

\begin_layout Paragraph
Equation 2
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathbf{y}_{0}+ha_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}=\mathbf{y}_{0}+\widetilde{a}_{21}\mathbf{\widetilde{k}}_{1}
\end{equation}

\end_inset

 
\begin_inset Formula 
\begin{equation}
\widetilde{a}_{21}=a_{21}
\end{equation}

\end_inset

 
\end_layout

\begin_layout Paragraph
Equation 3
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathbf{y}_{0}+ha_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}+ha_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\mathbf{k}_{1}\right)=\mathbf{y}_{0}+\widetilde{a}_{31}\mathbf{\widetilde{k}}_{1}+\widetilde{a}_{32}\mathbf{\widetilde{k}}_{2}
\end{equation}

\end_inset

 
\begin_inset Formula 
\begin{equation}
\mathbf{y}_{0}+ha_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}+ha_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)=\mathbf{y}_{0}+\widetilde{a}_{31}\mathbf{\widetilde{k}}_{1}+\widetilde{a}_{32}\mathbf{\widetilde{k}}_{2}
\end{equation}

\end_inset

 
\begin_inset Formula 
\begin{equation}
\mathbf{y}_{0}+\left(a_{31}+a_{32}c_{21}\right)\mathbf{\widetilde{k}}_{1}+a_{32}\mathbf{\widetilde{k}}_{2}=\mathbf{y}_{0}+\widetilde{a}_{31}\mathbf{\widetilde{k}}_{1}+\widetilde{a}_{32}\mathbf{\widetilde{k}}_{2}
\end{equation}

\end_inset

 
\begin_inset Formula 
\begin{equation}
\widetilde{a}_{31}=a_{31}+a_{32}c_{21}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\widetilde{a}_{32}=a_{32}
\end{equation}

\end_inset


\end_layout

\begin_layout Paragraph
Equation 4
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline}
\mathbf{y}_{0}+ha_{41}\frac{\mathbf{\widetilde{k}}_{1}}{h}+ha_{42}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\mathbf{k}_{1}\right)+ha_{43}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{31}\mathbf{k}_{1}+c_{32}\mathbf{k}_{2}\right)\\
=\mathbf{y}_{0}+\widetilde{a}_{41}\mathbf{\widetilde{k}}_{1}+\widetilde{a}_{42}\mathbf{\widetilde{k}}_{2}+\widetilde{a}_{43}\mathbf{\widetilde{k}}_{3}
\end{multline}

\end_inset

 
\begin_inset Formula 
\begin{multline}
\mathbf{y}_{0}+ha_{41}\frac{\mathbf{\widetilde{k}}_{1}}{h}+ha_{42}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)+ha_{43}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\mathbf{k}_{1}\right)\right)\\
=\mathbf{y}_{0}+\widetilde{a}_{41}\mathbf{\widetilde{k}}_{1}+\widetilde{a}_{42}\mathbf{\widetilde{k}}_{2}+\widetilde{a}_{43}\mathbf{\widetilde{k}}_{3}
\end{multline}

\end_inset

 
\begin_inset Formula 
\begin{multline}
\mathbf{y}_{0}+ha_{41}\frac{\mathbf{\widetilde{k}}_{1}}{h}+ha_{42}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)+ha_{43}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)\right)\\
=\mathbf{y}_{0}+\widetilde{a}_{41}\mathbf{\widetilde{k}}_{1}+\widetilde{a}_{42}\mathbf{\widetilde{k}}_{2}+\widetilde{a}_{43}\mathbf{\widetilde{k}}_{3}
\end{multline}

\end_inset

 
\begin_inset Formula 
\begin{multline}
\mathbf{y}_{0}+\left(a_{41}+a_{42}c_{21}+a_{43}c_{32}c_{21}\right)\mathbf{\widetilde{k}}_{1}+\left(a_{42}+a_{43}c_{32}\right)\mathbf{\widetilde{k}}_{2}+a_{43}\mathbf{\widetilde{k}}_{3}\\
=\mathbf{y}_{0}+\widetilde{a}_{41}\mathbf{\widetilde{k}}_{1}+\widetilde{a}_{42}\mathbf{\widetilde{k}}_{2}+\widetilde{a}_{43}\mathbf{\widetilde{k}}_{3}
\end{multline}

\end_inset

 
\begin_inset Formula 
\begin{equation}
\widetilde{a}_{41}=a_{41}+a_{42}c_{21}+a_{43}c_{32}c_{21}+a_{43}c_{31}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
