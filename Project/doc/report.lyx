\begin_inset Formula 
\begin{equation}
h\sum_{i=1}^{s}m_{i}\mathbf{k}_{i}=\sum_{i=1}^{s}\widetilde{m}_{i}\widetilde{\mathbf{k}}_{i}
\end{equation}

\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
h\left(m_{1}\mathbf{k}_{1}+m_{2}\mathbf{k}_{2}+m_{3}\mathbf{k}_{3}+m_{4}\mathbf{k}_{4}\right)=\widetilde{m}_{1}\widetilde{\mathbf{k}}_{1}+\widetilde{m}_{2}\widetilde{\mathbf{k}}_{2}+\widetilde{m}_{3}\widetilde{\mathbf{k}}_{3}+\widetilde{m}_{4}\widetilde{\mathbf{k}}_{4}
\end{equation}

\end_inset

 
\begin_inset Formula 
\begin{multline}
h\left(m_{1}\frac{\mathbf{\widetilde{k}}_{1}}{h}+m_{2}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\mathbf{k}_{1}\right)+m_{3}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{31}\mathbf{k}_{1}+c_{32}\mathbf{k}_{2}\right)+m_{4}\left(\frac{\mathbf{\widetilde{k}}_{4}}{h}+c_{41}\mathbf{k}_{1}+c_{42}\mathbf{k}_{2}+c_{43}\mathbf{k}_{3}\right)\right)\\
=\widetilde{m}_{1}\widetilde{\mathbf{k}}_{1}+\widetilde{m}_{2}\widetilde{\mathbf{k}}_{2}+\widetilde{m}_{3}\widetilde{\mathbf{k}}_{3}+\widetilde{m}_{4}\widetilde{\mathbf{k}}_{4}
\end{multline}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline}
h\left(m_{1}\frac{\mathbf{\widetilde{k}}_{1}}{h}+m_{2}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)+m_{3}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\mathbf{k}_{1}\right)\right)\right.\\
\left.+m_{4}\left(\frac{\mathbf{\widetilde{k}}_{4}}{h}+c_{41}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{42}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\mathbf{k}_{1}\right)+c_{43}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{31}\mathbf{k}_{1}+c_{32}\mathbf{k}_{2}\right)\right)\right)\\
=\widetilde{m}_{1}\widetilde{\mathbf{k}}_{1}+\widetilde{m}_{2}\widetilde{\mathbf{k}}_{2}+\widetilde{m}_{3}\widetilde{\mathbf{k}}_{3}+\widetilde{m}_{4}\widetilde{\mathbf{k}}_{4}
\end{multline}

\end_inset

 
\begin_inset Formula 
\begin{multline}
h\left(m_{1}\frac{\mathbf{\widetilde{k}}_{1}}{h}+m_{2}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)+m_{3}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)\right)\right.\\
\left.+m_{4}\left(\frac{\mathbf{\widetilde{k}}_{4}}{h}+c_{41}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{42}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)+c_{43}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)\right)\right)\right)\\
=\widetilde{m}_{1}\widetilde{\mathbf{k}}_{1}+\widetilde{m}_{2}\widetilde{\mathbf{k}}_{2}+\widetilde{m}_{3}\widetilde{\mathbf{k}}_{3}+\widetilde{m}_{4}\widetilde{\mathbf{k}}_{4}
\end{multline}

\end_inset

 
\begin_inset Formula 
\begin{multline}
\left(m_{1}+m_{2}c_{21}+m_{3}c_{31}+m_{3}c_{32}c_{21}+m_{4}c_{41}+m_{4}c_{42}c_{21}+m_{4}c_{43}c_{31}+m_{4}c_{43}c_{32}c_{21}\right)\mathbf{\widetilde{k}}_{1}\\
+\left(m_{2}+m_{3}c_{32}+m_{4}c_{42}+m_{4}c_{43}c_{32}\right)\mathbf{\widetilde{k}}_{2}+\left(m_{3}+m_{4}c_{43}\right)\mathbf{\widetilde{k}}_{3}+m_{4}\mathbf{\widetilde{k}}_{4}\\
=\widetilde{m}_{1}\widetilde{\mathbf{k}}_{1}+\widetilde{m}_{2}\widetilde{\mathbf{k}}_{2}+\widetilde{m}_{3}\widetilde{\mathbf{k}}_{3}+\widetilde{m}_{4}\widetilde{\mathbf{k}}_{4}
\end{multline}

\end_inset

 The coefficients are then related as:
