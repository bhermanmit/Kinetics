\end_inset

 
\begin_inset Formula 
\begin{equation}
\gamma h\left(c_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\mathbf{k}_{1}\right)\right)=-h\left(\widetilde{c}_{31}\widetilde{\mathbf{k}}_{1}+\widetilde{c}_{32}\widetilde{\mathbf{k}}_{2}\right)
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\gamma h\left(c_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\left(\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)\right)\right)=-h\left(\widetilde{c}_{31}\widetilde{\mathbf{k}}_{1}+\widetilde{c}_{32}\widetilde{\mathbf{k}}_{2}\right)
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\gamma h\left(c_{31}+c_{32}c_{21}\right)\mathbf{\widetilde{k}}_{1}+\gamma hc_{32}\mathbf{\widetilde{k}}_{2}=-h\widetilde{c}_{31}\widetilde{\mathbf{k}}_{1}-h\widetilde{c}_{32}\widetilde{\mathbf{k}}_{2}
