\gamma h\left(c_{41}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{42}\mathbf{k}_{2}+c_{43}\mathbf{k}_{3}\right)=-h\left(\widetilde{c}_{41}\widetilde{\mathbf{k}}_{1}+\widetilde{c}_{42}\widetilde{\mathbf{k}}_{2}+\widetilde{c}_{43}\widetilde{\mathbf{k}}_{3}\right)
\end{equation}

\end_inset

 
\begin_inset Formula 
\begin{equation}
\gamma h\left(c_{41}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{42}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\mathbf{k}_{1}\right)+c_{43}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{32}\mathbf{k}_{2}+c_{31}\mathbf{k}_{1}\right)\right)=-h\left(\widetilde{c}_{41}\widetilde{\mathbf{k}}_{1}+\widetilde{c}_{42}\widetilde{\mathbf{k}}_{2}+\widetilde{c}_{43}\widetilde{\mathbf{k}}_{3}\right)
\end{equation}

\end_inset


\begin_inset Formula 
\begin{multline}
\gamma h\left(c_{41}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{42}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)+c_{43}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\mathbf{k}_{1}\right)+c_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)\right)\\
=-h\left(\widetilde{c}_{41}\widetilde{\mathbf{k}}_{1}+\widetilde{c}_{42}\widetilde{\mathbf{k}}_{2}+\widetilde{c}_{43}\widetilde{\mathbf{k}}_{3}\right)
\end{multline}

\end_inset


\begin_inset Formula 
\begin{multline}
\gamma h\left(c_{41}\frac{\mathbf{\widetilde{k}}_{1}}{h}+c_{42}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)+c_{43}\left(\frac{\mathbf{\widetilde{k}}_{3}}{h}+c_{32}\left(\frac{\mathbf{\widetilde{k}}_{2}}{h}+c_{21}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)+c_{31}\frac{\mathbf{\widetilde{k}}_{1}}{h}\right)\right)\\
=-h\left(\widetilde{c}_{41}\widetilde{\mathbf{k}}_{1}+\widetilde{c}_{42}\widetilde{\mathbf{k}}_{2}+\widetilde{c}_{43}\widetilde{\mathbf{k}}_{3}\right)
\end{multline}

\end_inset

 
\begin_inset Formula 
\begin{equation}
\gamma h\left(c_{41}+c_{42}c_{21}+c_{43}c_{31}+c_{43}c_{32}c_{21}\right)\mathbf{\widetilde{k}}_{1}+\gamma h\left(c_{42}+c_{43}c_{32}\right)\mathbf{\widetilde{k}}_{2}+c_{43}\mathbf{\widetilde{k}}_{3}=-h\widetilde{c}_{41}\widetilde{\mathbf{k}}_{1}-h\widetilde{c}_{42}\widetilde{\mathbf{k}}_{2}-h\widetilde{c}_{43}\widetilde{\mathbf{k}}_{3}
\end{equation}

\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\widetilde{c}_{41}=-\gamma\left(c_{41}+c_{42}c_{21}+c_{43}c_{31}+c_{43}c_{32}c_{21}\right)
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\widetilde{c}_{42}=-\gamma\left(c_{42}+c_{43}c_{32}\right)
