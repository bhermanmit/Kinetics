\begin_layout Subsection
Convert from FORM 2 to FORM 3
\end_layout

\begin_layout Standard
FORM 3 of the equations are the set that will be implemented.
 To convert from FORM 2 where the coefficients are generated to FORM 3 we
 equate the equations and match parameters.
 The relation of the unknown vectors are 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\widetilde{\mathbf{k}}_{i}=\frac{\mathbf{\hat{k}}_{i}}{\gamma}+\sum_{j}^{i-1}\frac{\widetilde{c}_{ij}}{\gamma}\mathbf{\widetilde{k}}_{i}.
\end{equation}

\end_inset

 First, the 
\begin_inset Formula $a$
\end_inset

 coefficients are determined by equating the following from Eqs.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:form2k"

\end_inset

 and 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:form3k"

\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sum_{j=1}^{i-1}\widetilde{a}_{ij}\widetilde{\mathbf{k}}_{j}=\sum_{j=1}^{i-1}\widehat{a}_{ij}\mathbf{\hat{k}}_{j}.
\]

\end_inset

 
\end_layout

\begin_layout Paragraph
Equation 1
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
0=0
\]

\end_inset

 
\end_layout

\begin_layout Paragraph
Equation 2
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\widetilde{a}_{21}\widetilde{\mathbf{k}}_{1}=\widehat{a}_{21}\mathbf{\hat{k}}_{1}
\]

\end_inset


\begin_inset Formula 
\[
\widetilde{a}_{21}\frac{\hat{\mathbf{k}}_{1}}{\gamma}=\widehat{a}_{21}\gamma\hat{\mathbf{k}}_{1}
\]

\end_inset


\begin_inset Formula 
\[
\widehat{a}_{21}=\frac{\widetilde{a}_{21}}{\gamma}
\]

\end_inset


\end_layout

\begin_layout Paragraph
Equation 3
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\widetilde{a}_{31}\widetilde{\mathbf{k}}_{1}+\widetilde{a}_{32}\widetilde{\mathbf{k}}_{2}=\widehat{a}_{31}\mathbf{\hat{k}}_{1}+\widehat{a}_{32}\mathbf{\hat{k}}_{2}
\]

\end_inset


\begin_inset Formula 
\[
\widetilde{a}_{31}\frac{\hat{\mathbf{k}}_{1}}{\gamma}+\widetilde{a}_{32}\left(\frac{\hat{\mathbf{k}}_{2}}{\gamma}+\frac{\widetilde{c}_{21}}{\gamma}\widetilde{\mathbf{k}}_{1}\right)=\widehat{a}_{31}\mathbf{\hat{k}}_{1}+\widehat{a}_{32}\mathbf{\hat{k}}_{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\widetilde{a}_{31}\frac{\hat{\mathbf{k}}_{1}}{\gamma}+\widetilde{a}_{32}\left(\frac{\hat{\mathbf{k}}_{2}}{\gamma}+\frac{\widetilde{c}_{21}}{\gamma}\frac{\hat{\mathbf{k}}_{1}}{\gamma}\right)=\widehat{a}_{31}\mathbf{\hat{k}}_{1}+\widehat{a}_{32}\mathbf{\hat{k}}_{2}
\]

\end_inset

 
\begin_inset Formula 
\[
\left(\frac{\widetilde{a}_{31}}{\gamma}+\frac{\widetilde{c}_{21}}{\gamma^{2}}\right)\hat{\mathbf{k}}_{1}+\left(\frac{\widetilde{a}_{32}}{\gamma}\right)\hat{\mathbf{k}}_{2}=\widehat{a}_{31}\mathbf{\hat{k}}_{1}+\widehat{a}_{32}\mathbf{\hat{k}}_{2}
\]

\end_inset

 
\begin_inset Formula 
\[
\widehat{a}_{31}=\frac{\widetilde{a}_{31}}{\gamma}+\frac{\widetilde{a}_{32}\widetilde{c}_{21}}{\gamma^{2}}
\]

\end_inset


\begin_inset Formula 
\[
\widehat{a}_{32}=\frac{\widetilde{a}_{32}}{\gamma}
\]

\end_inset

 
\end_layout

\begin_layout Paragraph
Equation 4
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\widetilde{a}_{41}\widetilde{\mathbf{k}}_{1}+\widetilde{a}_{42}\widetilde{\mathbf{k}}_{2}+\widetilde{a}_{43}\widetilde{\mathbf{k}}_{3}=\widehat{a}_{41}\mathbf{\hat{k}}_{1}+\widehat{a}_{42}\mathbf{\hat{k}}_{2}+\widehat{a}_{43}\mathbf{\hat{k}}_{3}
\]

\end_inset


\begin_inset Formula 
\[
\widetilde{a}_{41}\frac{\hat{\mathbf{k}}_{1}}{\gamma}+\widetilde{a}_{42}\left(\frac{\hat{\mathbf{k}}_{2}}{\gamma}+\frac{\widetilde{c}_{21}}{\gamma}\widetilde{\mathbf{k}}_{1}\right)+\widetilde{a}_{43}\left(\frac{\hat{\mathbf{k}}_{3}}{\gamma}+\frac{\widetilde{c}_{31}}{\gamma}\widetilde{\mathbf{k}}_{1}+\frac{\widetilde{c}_{32}}{\gamma}\widetilde{\mathbf{k}}_{2}\right)=\widehat{a}_{41}\mathbf{\hat{k}}_{1}+\widehat{a}_{42}\mathbf{\hat{k}}_{2}+\widehat{a}_{43}\mathbf{\hat{k}}_{3}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\widetilde{a}_{41}\frac{\hat{\mathbf{k}}_{1}}{\gamma}+\widetilde{a}_{42}\left(\frac{\hat{\mathbf{k}}_{2}}{\gamma}+\frac{\widetilde{c}_{21}}{\gamma}\frac{\hat{\mathbf{k}}_{1}}{\gamma}\right)+\widetilde{a}_{43}\left(\frac{\hat{\mathbf{k}}_{3}}{\gamma}+\frac{\widetilde{c}_{31}}{\gamma}\frac{\hat{\mathbf{k}}_{1}}{\gamma}+\frac{\widetilde{c}_{32}}{\gamma}\left(\frac{\hat{\mathbf{k}}_{2}}{\gamma}+\frac{\widetilde{c}_{21}}{\gamma}\widetilde{\mathbf{k}}_{1}\right)\right)=\widehat{a}_{41}\mathbf{\hat{k}}_{1}+\widehat{a}_{42}\mathbf{\hat{k}}_{2}+\widehat{a}_{43}\mathbf{\hat{k}}_{3}
\]

\end_inset


\begin_inset Formula 
\[
\widetilde{a}_{41}\frac{\hat{\mathbf{k}}_{1}}{\gamma}+\widetilde{a}_{42}\left(\frac{\hat{\mathbf{k}}_{2}}{\gamma}+\frac{\widetilde{c}_{21}}{\gamma}\frac{\hat{\mathbf{k}}_{1}}{\gamma}\right)+\widetilde{a}_{43}\left(\frac{\hat{\mathbf{k}}_{3}}{\gamma}+\frac{\widetilde{c}_{31}}{\gamma}\frac{\hat{\mathbf{k}}_{1}}{\gamma}+\frac{\widetilde{c}_{32}}{\gamma}\left(\frac{\hat{\mathbf{k}}_{2}}{\gamma}+\frac{\widetilde{c}_{21}}{\gamma}\frac{\hat{\mathbf{k}}_{1}}{\gamma}\right)\right)=\widehat{a}_{41}\mathbf{\hat{k}}_{1}+\widehat{a}_{42}\mathbf{\hat{k}}_{2}+\widehat{a}_{43}\mathbf{\hat{k}}_{3}
\]

\end_inset

 
\begin_inset Formula 
\[
\left(\frac{\widetilde{a}_{41}}{\gamma}+\frac{\widetilde{a}_{42}\widetilde{c}_{21}}{\gamma^{2}}+\frac{\widetilde{a}_{43}\widetilde{c}_{31}}{\gamma^{2}}+\frac{\widetilde{a}_{43}\widetilde{c}_{32}\widetilde{c}_{21}}{\gamma^{3}}\right)\mathbf{\hat{k}}_{1}+\left(\frac{\widetilde{a}_{42}}{\gamma}+\frac{\widetilde{a}_{43}\widetilde{c}_{32}}{\gamma^{2}}\right)\mathbf{\hat{k}}_{2}+\left(\frac{\widetilde{a}_{43}}{\gamma}\right)\mathbf{\hat{k}}_{3}=\widehat{a}_{41}\mathbf{\hat{k}}_{1}+\widehat{a}_{42}\mathbf{\hat{k}}_{2}+\widehat{a}_{43}\mathbf{\hat{k}}_{3}
\]

\end_inset


\begin_inset Formula 
\[
\widehat{a}_{41}=\frac{\widetilde{a}_{41}}{\gamma}+\frac{\widetilde{a}_{42}\widetilde{c}_{21}}{\gamma^{2}}+\frac{\widetilde{a}_{43}\widetilde{c}_{31}}{\gamma^{2}}+\frac{\widetilde{a}_{43}\widetilde{c}_{32}\widetilde{c}_{21}}{\gamma^{3}}
\]

\end_inset


\begin_inset Formula 
\[
\widehat{a}_{42}=\frac{\widetilde{a}_{42}}{\gamma}+\frac{\widetilde{a}_{43}\widetilde{c}_{32}}{\gamma^{2}}
\]

\end_inset

 
\begin_inset Formula 
\[
\widehat{a}_{43}=\frac{\widetilde{a}_{43}}{\gamma}
\]

\end_inset


\end_layout

